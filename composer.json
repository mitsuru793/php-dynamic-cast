{
    "name": "mitsuru793/php-dynamic-cast",
    "description": "You can cast value dynamically with helper function.",
    "type": "library",
    "require": {
        "tightenco/collect": "^5.5",
        "symfony/console": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "mikey179/vfsStream": "^1.6",
        "mihaeu/html-formatter": "^1.0",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PhpDynamicCast\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Helper\\": "tests/Helper"
        }
    }
}
